{
  "coc.source.file.ignorePatterns": [".turbo-cache"],
  "workspace.ignoredFolders": ["$HOME", ".turbo-cache"],
  "workspace.rootPatterns": [".git"],
  "snippets.ultisnips.directories": ["~/.config/nvim/UltiSnips"],
  "coc.source.buffer.ignoreGitignore": true,
  "tabnine.priority": 90,
  "suggest.languageSourcePriority": 100,
  "snippets.priority": 120,
  "tabnine.enablePreselect": false,
  "snippets.ultisnips.enable": true,
  "tailwindCSS.headwind.runOnSave": false,
  "coc.preferences.formatOnSave": true
}
